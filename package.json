{
  "name": "sakazuki",
  "private": true,
  "dependencies": {
    "@rails/ujs": "7.0.2-2",
    "bootstrap": ">=5.1.3 <6",
    "bootstrap-icons": ">=1.5.0",
    "chart.js": ">=2.9.4 <3",
    "esbuild": "^0.14.25",
    "rater-js": ">=1.0.1 <2",
    "simplelightbox": ">=2.9.0 <3"
  },
  "version": "0.1.0",
  "devDependencies": {
    "@types/chart.js": ">=2.9.34 <3",
    "@typescript-eslint/eslint-plugin": ">=4.32.0",
    "@typescript-eslint/parser": ">=4.32.0",
    "eslint": ">=7.32.0",
    "eslint-config-prettier": ">=8.3.0",
    "markdownlint-cli": ">=0.28.1",
    "prettier": ">=2.4.1",
    "stylelint": ">=13.13.1",
    "stylelint-config-standard": ">=22.0.0",
    "stylelint-scss": ">=3.21.0",
    "typescript": "<4.5"
  },
  "scripts": {
    "yard": "yard doc --tag type:\"TYPE\" {lib,app,spec,ext}/**/*.rb",
    "lint:eslint": "eslint --ignore-path .prettierignore --ext .js,.ts .",
    "lint:eslint:fix": "yarn run lint:eslint --fix",
    "lint:prettier": "prettier --check .",
    "lint:prettier:fix": "prettier --write .",
    "lint:markdownlint": "markdownlint README.md",
    "lint:markdownlint:fix": "yarn run lint:markdownlint --fix",
    "lint:stylelint": "stylelint '**/*.scss' '**/*.css' --ignore-path .gitignore",
    "lint:stylelint:fix": "yarn run lint:stylelint --fix",
    "build": "esbuild app/javascript/*.* --bundle --sourcemap --outdir=app/assets/builds"
  },
  "packageManager": "yarn@3.1.1"
}
